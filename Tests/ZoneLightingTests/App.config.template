<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
	<appSettings>
		<add key="FadeCandyServerURL" value="[URL of server running FadeCandy - if one exists]"/>
		<add key="ProgramDLLFolder" value ="[Path of folder where all program modules are to be loaded from]"/>
		<add key="ZoneDLLFolder" value ="[Path of folder where all zone modules are to be loaded from]"/>
		<add key="ZoneConfigurationSaveFile" value ="[Path of file in which the zone configuration is to be saved]"/>
		<add key="TestZoneConfigurationSaveFile" value ="[Path of file in which the zone configuration is to be saved for testing]"/>
		<add key="TestProgramModuleDirectory" value ="[Path of folder in which the program modules for testing are stored]"/>
	</appSettings>
</configuration>